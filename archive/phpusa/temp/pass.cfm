		<script language="javascript">

        var ieDOM = false, nsDOM = false;
        var stdDOM = document.getElementById; 
        
        function getObject(objectId)
        {
            if (stdDOM) return document.getElementById(objectId);
            if (ieDOM) return document.all[objectId];
            if (nsDOM) return document.layers[objectId];
        }

        function getObjectStyle(objectId)
        {
            if (nsDOM) return getObject(objectId);
            var obj = getObject(objectId);
            return obj.style;
        }

        function showDefault(objectId)
        {
            showCell(objectId, "silver", "white");
        }

        function showCell(objectId, foreColor, backColor)
        {
            getObjectStyle(objectId).color = foreColor;
            getObjectStyle(objectId).backgroundColor = backColor;
        }

        function showUnacceptable()
        {
            getObjectStyle("pwOuterTable").borderWidth = "0";
            getObjectStyle("pwTable").borderWidth = "1";
            getObjectStyle("pwWeak").borderRightColor = "silver";
            showCell("pwWeak", "white", "red");
            getObjectStyle("pwMedium").borderRightColor = "silver";
            showDefault("pwMedium");
            getObjectStyle("pwStrong").fontWeight = "normal";
            showDefault("pwStrong");
        }

        function showAcceptable()
        {
            getObjectStyle("pwOuterTable").borderWidth = "0";
            getObjectStyle("pwTable").borderWidth = "1";
            getObjectStyle("pwWeak").borderRightColor = "lightgreen";
            showCell("pwWeak", "lightgreen", "lightgreen");
            getObjectStyle("pwMedium").borderRightColor = "silver";
            showCell("pwMedium", "black", "lightgreen");
            getObjectStyle("pwStrong").fontWeight = "normal";
            showDefault("pwStrong");
        }

        function showStrong()
        {
            getObjectStyle("pwOuterTable").borderWidth = "1";
            getObjectStyle("pwTable").borderWidth = "0";
            getObjectStyle("pwWeak").borderRightColor = "#29C72A";
            showCell("pwWeak", "#29C72A", "#29C72A");
            getObjectStyle("pwMedium").borderRightColor = "#29C72A";
            showCell("pwMedium", "#29C72A", "#29C72A");
            getObjectStyle("pwStrong").fontWeight = "bold";
            showCell("pwStrong", "white", "#29C72A");
        }

        function showUndetermined()
        {
            getObjectStyle("pwOuterTable").borderWidth = "0";
            getObjectStyle("pwTable").borderWidth = "1";
            getObjectStyle("pwWeak").borderRightColor = "silver";
            showDefault("pwWeak");
            getObjectStyle("pwMedium").borderRightColor = "silver";
            showDefault("pwMedium");
            getObjectStyle("pwStrong").fontWeight = "normal";
            showDefault("pwStrong");
        }

        function passwordChanged(obj)
        {
            var pwd = obj.value;

            if( pwd.length < 6 )
            {
                showUndetermined();
                getObjectStyle('lblPasswordValid3').display='none';
            }
            else if ( pwd.toLowerCase() == getObject('txtUsername').value.toLowerCase() || 
                        pwd.toLowerCase() == getObject('txtNickname').value.toLowerCase() )
            {
                showUnacceptable();
                getObjectStyle('lblPasswordReq').display='none';
                getObjectStyle('lblPasswordValid1').display='none';
                getObjectStyle('lblPasswordValid2').display='none';
                getObjectStyle('lblPasswordValid3').display='inline';
            }
            else if( isStrong(pwd) )
            {
                showStrong();
                getObjectStyle('lblPasswordValid3').display='none';
            }
            else if( isAcceptable( pwd ) )
            {
                showAcceptable();
                getObjectStyle('lblPasswordValid3').display='none';
            }                
            else
            {
                showUnacceptable();
                getObjectStyle('lblPasswordValid3').display='none';
            }
        }
        
        function isStrong(str) {
            if (numDigits(str) >= 2 && numChars(str) >= 2 && (str.length >= 10 || numSpecialChars(str) >= 1))
                return true;
            else
                return false;
        }
        
        function isAcceptable(str) {
            if (numDigits(str) >= 2 && numChars(str) >= 2)
                return true;
            else
                return false;
        }

        function numChars(str) {
            var charCount=0;
            
            for (i=0; i < str.length; i++)
                if (isChar(str.charAt(i)))
                    charCount ++;
            
            return charCount;
        }
        
        function numDigits(str) {
            var charCount=0;
            
            for (i=0; i < str.length; i++)
                if (isDigit(str.charAt(i)))
                    charCount ++;
            
            return charCount;
        }

        function numSpecialChars(str) {
            var charCount=0;
            
            for (i=0; i < str.length; i++)
                if (isSpecialChar(str.charAt(i)))
                    charCount ++;
            
            return charCount;
        }

        function isChar(ch)
        {
            var  regExText, regExp;

            regExText  = '[A-Za-z]';
            regExp    = new RegExp(regExText);
            return regExp.test(ch);
        }
        
        function isDigit(ch) {
            var  regExText, regExp;

            regExText  = '[0-9]';
            regExp    = new RegExp(regExText);
            return regExp.test(ch);
        }
        
        function isSpecialChar(ch){
            if (!isDigit(ch) && !isChar(ch))
                return true;
            else
                return false;
        }
        
        function trim(str)
        {
            return str.replace(/^\s*|\s*$/g,"");
        }

        function CheckAvailability(sFeature, sName)
        {
            sName=trim(sName);

            //Disable appropriate command button
            getObject('btn' + sFeature + 'Avail').disabled='true';
            
            if (sName=='')
            {
                getObjectStyle('lbl' + sFeature + 'Req').display='inline';
                getObjectStyle('lbl' + sFeature + 'Valid').display='none';
                getObjectStyle('lbl' + sFeature + 'Valid1').display='none';
	            getObject('lbl' + sFeature + 'Ind').innerHTML="<img src='/images/reg-alert.gif' height='16' width='16' border='0'>";
	            getObject('lbl' + sFeature).className='errorLabel';
	            getObject('txt' + sFeature).className='errorInputUsername';
	            getObject('txt' + sFeature).value='';
	            getObject('txt' + sFeature).focus();

                //Enable appropriate command button
                getObject('btn' + sFeature + 'Avail').disabled='';
	            return;
            }
            
            if (sName.indexOf('<') != -1 || sName.indexOf('>') != -1)
            {
                getObjectStyle('lbl' + sFeature + 'Req').display='none';
                getObjectStyle('lbl' + sFeature + 'Valid').display='none';
                getObjectStyle('lbl' + sFeature + 'Valid1').display='inline';
	            getObject('lbl' + sFeature + 'Ind').innerHTML="<img src='/images/reg-alert.gif' height='16' width='16' border='0'>";
	            getObject('lbl' + sFeature).className='errorLabel';
	            getObject('txt' + sFeature).className='errorInputUsername';

                //Enable appropriate command button
                getObject('btn' + sFeature + 'Avail').disabled='';
	            return;
            }
            else
            {
                // Encode the input
                sName = escape(sName);
                
                // *************** Creating XMLHTTP Object *************
                var xmlhttp=false;
                var xmlHttpRequest=false;

                try {                                                                    //For IE.,
                    xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
                } catch (e) {
                try {
                    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
                } catch (E) {
                    xmlhttp = false;
                }
                }

                // *************** Creating XMLHTTPRequest Object *************
                if (!xmlhttp && typeof XMLHttpRequest!='undefined') {                   //For Mozilla etc.,
	                try {
		                xmlhttp = new XMLHttpRequest();
	                } catch (e) {
		                xmlhttp=false;
	                }
                }
                if (!xmlhttp && window.createRequest) {
	                try {
		                xmlhttp = window.createRequest();
	                } catch (e) {
		                xmlhttp=false;
	                }
                }

                // *************** Making the request *************
                xmlhttp.open("GET", "/service/web.dll?request=CheckFeatureAvailability&" + sFeature + "=" + sName, true);
                xmlhttp.onreadystatechange=function() {
                    if (xmlhttp.readyState==4) {
                        //Parse the result from CAS, to get result code
                        var xmlDoc;
                        try {
                            xmlDoc = new ActiveXObject("Microsoft.XMLDOM");                 //For IE
                            xmlDoc.async="false";
                            xmlDoc.loadXML(xmlhttp.responseText);
                        } catch (e) {
                            xmlDoc= document.implementation.createDocument("","",null);     //For Mozilla etc.,
                            var parser = new DOMParser(); 
                            xmlDoc = parser.parseFromString(xmlhttp.responseText, "text/xml"); 
                        }
                        
                        var responseCode=xmlDoc.documentElement.getAttribute("rc");
                        
                        if(responseCode=='0')
                        {
                            getObjectStyle('lbl' + sFeature + 'Req').display='none';
                            getObjectStyle('lbl' + sFeature + 'Valid').display='inline';
	                        getObject('lbl' + sFeature + 'Valid').innerHTML= "<strong>" + unescape(sName) + "</strong> is available";
                            getObjectStyle('lbl' + sFeature + 'Valid1').display='none';
	                        getObject('lbl' + sFeature + 'Ind').innerHTML="*";
	                        getObjectStyle('lbl' + sFeature + 'Ind').color="#a3a3a3";
	                        getObjectStyle('lbl' + sFeature + 'Ind').fontWeight="normal";
	                        getObject('lbl' + sFeature).className='plainLabel';
	                        getObject('txt' + sFeature).className='normalInputUsername';
	                        
	                        if (sFeature == 'Username')
	                            getObject('txtPassword').focus();
	                        else
	                            getObject('txtReferralCode').focus();

                            //Enable appropriate command button
                            getObject('btn' + sFeature + 'Avail').disabled='';
	                    }
                        else
                        {
                            getObjectStyle('lbl' + sFeature + 'Req').display='none';
                            getObjectStyle('lbl' + sFeature + 'Valid').display='inline';
                            getObject('lbl' + sFeature + 'Valid').innerHTML = "<strong>" + unescape(sName) + "</strong> is not available.<br>Please try a different selection";
                            getObjectStyle('lbl' + sFeature + 'Valid1').display='none';
	                        getObject('lbl' + sFeature + 'Ind').innerHTML= "<img src='/images/reg-alert.gif' height='16' width='16' border='0'>";
	                        getObject('lbl' + sFeature).className='errorLabel';
	                        getObject('txt' + sFeature).className='errorInputUsername';
	                        getObject('txt' + sFeature).focus();

                            //Enable appropriate command button
                            getObject('btn' + sFeature + 'Avail').disabled='';
	                    }
                    }
                }
                xmlhttp.send(null);
            }
        }
		</script>
